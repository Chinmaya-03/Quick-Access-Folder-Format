<Page
    x:Class="tty.Videos"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:tty"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <HyperlinkButton  NavigateUri="https://www.youtube.com/" Margin="20,123,0,332" Height="265">
            <Image x:ConnectionId='7' x:Name="image" Source="Image/videoyoutube.png" HorizontalAlignment="Left" Margin="0,0,0,-27" Width="424" NineGrid="0"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="https://www.netflix.com/" Margin="45,418,0,30" Height="272" Width="421">
            <Image x:ConnectionId='6' x:Name="image3" Source="Image/videonetflix.png" HorizontalAlignment="Left" Height="259" Margin="-34,0,0,-12" VerticalAlignment="Top" Width="410"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="http://www.dailymotion.com/in" Margin="955,123,0,354" Height="243" Width="268">
            <Image x:ConnectionId='5' x:Name="image4" Source="Image/videodmotion.png" Height="217" Margin="-59,0,-21,0" VerticalAlignment="Top"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="https://vimeo.com/" Margin="487,0,0,239" VerticalAlignment="Bottom">
            <Image x:ConnectionId='4' x:Name="image5" Source="Image/videovimeo.png" HorizontalAlignment="Left" Height="160.578" Margin="0" VerticalAlignment="Top" Width="426.219" RenderTransformOrigin="0.5,0.5" UseLayoutRounding="False" d:LayoutRounding="Auto">
                <Image.RenderTransform>
                    <CompositeTransform Rotation="-1.105" SkewY="1.202" TranslateX="0.086" TranslateY="4.468"/>
                </Image.RenderTransform>
            </Image>
        </HyperlinkButton>
        <HyperlinkButton  NavigateUri="https://in.screen.yahoo.com/" Margin="949,409,0,12" Height="299">
            <Image x:ConnectionId='3' x:Name="image7" Source="Image/videoyscreen.png" HorizontalAlignment="Left" Height="272" Margin="0" VerticalAlignment="Top" Width="314"/>
        </HyperlinkButton>
        <Button x:ConnectionId='1' x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="1144,10,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="29.333" Width="126"                     />
        <TextBlock x:ConnectionId='2' x:Name="textBlock" HorizontalAlignment="Left" Margin="583,10,0,0" TextWrapping="Wrap" Text="Videos" VerticalAlignment="Top" FontFamily="Century" FontSize="73.333"/>

    </Grid>
</Page>

