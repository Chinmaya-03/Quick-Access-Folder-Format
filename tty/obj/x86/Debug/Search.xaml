<Page
    x:Class="tty.Search"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:tty"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:ConnectionId='1' x:Name="textBlock" HorizontalAlignment="Left" Margin="333,10,0,0" TextWrapping="Wrap" Text="Search Engines" VerticalAlignment="Top" FontFamily="Century" FontSize="80"/>
        <HyperlinkButton NavigateUri="https://www.bing.com/" Margin="783,176,0,329" Height="215" Width="437">
            <Image x:ConnectionId='6' x:Name="image2" Source="Image/bing.png" HorizontalAlignment="Left" Height="200" Margin="0" VerticalAlignment="Top" Width="415"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="https://www.google.co.in/" Margin="94,396,0,20" Width="388">
            <Image x:ConnectionId='5' x:Name="image6" Source="Image/google.jpg" HorizontalAlignment="Left" Height="292" Margin="21,0,-22,0" VerticalAlignment="Top" Width="344"/>
        </HyperlinkButton>
        <HyperlinkButton  NavigateUri="https://in.search.yahoo.com/" Margin="94,159,0,329">
            <Image x:ConnectionId='4' x:Name="image8" Source="Image/yahoo.jpg" HorizontalAlignment="Left" Height="220" Margin="0" VerticalAlignment="Top" Width="417"/>
        </HyperlinkButton>

        <HyperlinkButton  NavigateUri="http://www.ask.com/" Margin="805,396,0,-6">
            <Image x:ConnectionId='3' x:Name="image3" Source="Image/ask.jpg" HorizontalAlignment="Left" Height="268" Margin="0" VerticalAlignment="Top" Width="413"/>
        </HyperlinkButton>
        <Button x:ConnectionId='2' x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="1144,10,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="29.333" Width="126"                     />

    </Grid>
</Page>

