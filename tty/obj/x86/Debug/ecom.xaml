<Page
    x:Class="tty.ecom"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:tty"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

        <HyperlinkButton NavigateUri="http://www.amazon.in/" Margin="47,72,0,495" HorizontalAlignment="Left" VerticalAlignment="Stretch" Width="371">
            <Image x:ConnectionId='10' x:Name="image1" Source="Image/ecamazon.jpg" HorizontalAlignment="Left" Height="139" Margin="-16,0,-59,0" VerticalAlignment="Top" Width="446" RenderTransformOrigin="0.397,-0.119"/>
        </HyperlinkButton>
        <HyperlinkButton  NavigateUri="http://www.ebay.in/" Margin="47,280,0,201" Height="239" Width="333">
            <Image x:ConnectionId='9' x:Name="image" Source="Image/ecebay.jpeg" HorizontalAlignment="Left" Height="223" Margin="50,0,0,0" VerticalAlignment="Top" Width="251"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="http://www.shimply.com/" Margin="10,556,0,10" Height="154" Width="408">
            <Image x:ConnectionId='8' x:Name="image3" Source="Image/ecshimply.png" HorizontalAlignment="Left" Height="134" Margin="0" VerticalAlignment="Top" Width="366"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="http://www.jabong.com/online-sale/" Margin="860,280,0,201" Height="239" Width="412">
            <Image x:ConnectionId='7' x:Name="image4" Source="Image/ecjabong.png" HorizontalAlignment="Left" Height="223" Margin="50,0,0,0" VerticalAlignment="Top" Width="352"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="http://www.shopclues.com/" Margin="860,575,0,10" Height="135" Width="412">
            <Image x:ConnectionId='6' x:Name="image5" Source="Image/ecshopclues.png" HorizontalAlignment="Left" Height="118" Margin="-28,-17,0,0" VerticalAlignment="Top" Width="430" RenderTransformOrigin="0.5,0.5"/>
        </HyperlinkButton>
        <HyperlinkButton NavigateUri="http://www.snapdeal.com/" Margin="927,72,0,495" Height="153" Width="317">
            <Image x:ConnectionId='5' x:Name="image6" Source="Image/ecsnapdeal.jpg" HorizontalAlignment="Left" Height="147" Margin="0,0,0,-6" VerticalAlignment="Top" Width="278"/>
        </HyperlinkButton>
        <Button x:ConnectionId='1' x:Name="button" Content="Back" HorizontalAlignment="Left" Margin="1144,10,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="29.333" Width="126"                     />
        <HyperlinkButton NavigateUri="http://www.flipkart.com/" Margin="486,72,0,495" Height="153" Width="390">
            <Image x:ConnectionId='4' x:Name="image2" Source="Image/ecflipkart.png" Height="154" Margin="0,0,-41,-13" VerticalAlignment="Top"/>
        </HyperlinkButton>
        <HyperlinkButton  NavigateUri="http://www.myntra.com/" Margin="419,556,0,10" Height="154" Width="432">
            <Image x:ConnectionId='3' x:Name="image7" Source="Image/ecmyntra.jpg" HorizontalAlignment="Left" Margin="-28,0,0,19" Width="383"/>
        </HyperlinkButton>
        <TextBlock x:ConnectionId='2' x:Name="textBlock" HorizontalAlignment="Left" Margin="419,351,0,0" TextWrapping="Wrap" Text="E Commerce" VerticalAlignment="Top" FontFamily="Century" FontSize="73.333"/>

    </Grid>
</Page>

